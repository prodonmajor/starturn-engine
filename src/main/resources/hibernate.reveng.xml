    </table>
    <table name="MEMBER_WALLET">
        <primary-key>
            <generator class="assigned"/>
            <key-column name="id" type="int"/>
        </primary-key>
    </table>
    <table name="PAYMENT_TYPE">
        <primary-key>
            <generator class="assigned"/>
            <key-column name="id" type="int"/>
        </primary-key>
    </table>
    <table name="INTEREST_DISBURSEMENT_TYPE">
