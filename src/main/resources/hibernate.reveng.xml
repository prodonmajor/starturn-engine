            <key-column name="id" type="int"/>
        </primary-key>
    </table>
    
    <table name="REQUIREMENT">
        <primary-key>
            <generator class="assigned"/>
            <key-column name="id" type="int"/>
        </primary-key>
    </table>
    
    <table name="ESUSU_GROUP_INVITES">
