            <key-column name="id" type="int"/>
        </primary-key>
    </table>
    
    <table name="MEMBER_PROFILE">
        <primary-key>
            <generator class="assigned"/>
            <key-column name="id" type="int"/>
        </primary-key>
    </table>
    
    <table name="TARGET_SAVINGS">
        <primary-key>
            <generator class="assigned"/>
            <key-column name="id" type="int"/>
        </primary-key>
    </table>
    
    <table name="ESUSU_GROUP">
        <primary-key>
            <generator class="assigned"/>
            <key-column name="id" type="int"/>
        </primary-key>
    </table>
    
    <table name="GENDER">
        <primary-key>
            <generator class="assigned"/>
            <key-column name="id" type="int"/>
        </primary-key>
    </table>
    
    <table name="TRANSACTION">
        <primary-key>
            <generator class="assigned"/>
            <key-column name="id" type="int"/>
        </primary-key>
    </table>
    
