<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
    <schema-selection match-catalog="starturn"/>
    
    <type-mapping>
        <sql-type jdbc-type="BIGINT" hibernate-type="java.lang.Long"/>
        <!--<sql-type jdbc-type="DECIMAL" hibernate-type="java.lang.Double"/>-->
    </type-mapping>
    
    <table-filter match-name="CONTRIBUTION_FREQUENCY"/>
    <table-filter match-name="TARGET_SAVINGS_PLAN"/>
    <table-filter match-name="ESUSU_GROUP_MEMBERS"/>
    <table-filter match-name="REQUIREMENT"/>
    <table-filter match-name="BANK"/>
    <table-filter match-name="ESUSU_GROUP_INVITES"/>
    <table-filter match-name="USER_TOKEN"/>
    <table-filter match-name="MEMBER_PROFILE"/>
    <table-filter match-name="TARGET_SAVINGS"/>
    <table-filter match-name="ESUSU_GROUP"/>
    <table-filter match-name="GENDER"/>
    <table-filter match-name="TRANSACTION"/>
    <table-filter match-name="TRANSACTION_TYPE"/>
    <table-filter match-name="MEMBER_WALLET_TRANSACTION"/>
    <table-filter match-name="MEMBER_WALLET"/>
    <table-filter match-name="PAYMENT_TYPE"/>
    <table-filter match-name="INTEREST_DISBURSEMENT_TYPE"/>
    <table-filter match-name="ESUSU_REPAYMENT_SCHEDULE"/>
    <table-filter match-name="OPERATOR_ESUSU_INTEREST" />
    <table-filter match-name="MEMBER_PROFILE_PICTURE"/>
  
    <table name="BANK">
        <primary-key>
            <generator class="assigned"/>
            <key-column name="id" type="int"/>
        </primary-key>
    </table>
    
    <table name="CONTRIBUTION_FREQUENCY">
        <primary-key>
            <generator class="assigned"/>
            <key-column name="id" type="int"/>
        </primary-key>
    </table>
    
